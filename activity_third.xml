<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/relativeLayout1"
    android:minWidth="25px"
    android:minHeight="25px"
    android:background="#ff536dfe">
    <LinearLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linearLayout1"
        android:layout_margin="15dp"
        android:background="#ffb0bec5">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="73.5dp"
            android:id="@+id/linearLayout2"
            android:gravity="center"
            android:background="#ffffffff">
            <TextView
                android:text="@string/ThirdPage"
                android:layout_width="match_parent"
                android:layout_height="52.0dp"
                android:id="@+id/textView1"
                android:background="#ff000000"
                android:textSize="30dp"
                android:textStyle="bold"
                android:textColor="#ffffffff"
                android:gravity="center"
                android:layout_margin="20dp" />
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout3"
            android:background="#ff78909c"
            android:gravity="center"
            android:layout_margin="10dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/username"
                android:background="#ffffffff"
                android:padding="10dp"
                android:text="@string/Username"
                android:textColor="#ff000000"
                android:textSize="17dp"
                android:textStyle="bold" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/password"
                android:background="#ffffffff"
                android:padding="10dp"
                android:text="@string/Password"
                android:textColor="#ff000000"
                android:textSize="17dp"
                android:textStyle="bold" />            
            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/mobile"
                android:background="#ffffffff"
                android:padding="10dp"
                android:text="@string/Mobile"
                android:textColor="#ff000000"
                android:textSize="17dp"
                android:textStyle="bold" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/email"
                android:background="#ffffffff"
                android:padding="10dp"
                android:text="@string/Email"
                android:textColor="#ff000000"
                android:textSize="17dp"
                android:textStyle="bold" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/latitude"
                android:background="#ffffffff"
                android:padding="10dp"
                android:text="@string/Latitude"
                android:textColor="#ff000000"
                android:textSize="17dp"
                android:textStyle="bold" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/longitude"
                android:background="#ffffffff"
                android:padding="10dp"
                android:text="@string/Longitude"
                android:textColor="#ff000000"
                android:textSize="17dp"
                android:textStyle="bold" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:id="@+id/linearLayout4"
            android:gravity="center"
            android:background="#ff8d6e63"
            android:layout_margin="5dp"
            android:layout_gravity="center">           
            <Button
                android:text="@string/Call"
                android:layout_width="100.0dp"
                android:layout_height="70dp"
                android:id="@+id/call"
                android:background="#ff000000"
                android:textColor="#ffffffff"
                android:textStyle="bold"
                android:textSize="17dp"
                android:gravity="center"
                android:layout_margin="5dp" /> 
            <Button
                android:text="@string/Email"
                android:layout_width="101.5dp"
                android:layout_height="70dp"
                android:id="@+id/mailme"
                android:background="#ff000000"
                android:textColor="#ffffffff"
                android:textStyle="bold"
                android:textSize="17dp"
                android:gravity="center"
                android:layout_margin="5dp" />
                <Button
                android:text="@string/Location"
                android:layout_width="100.0dp"
                android:layout_height="70dp"
                android:id="@+id/location"
                android:background="#ff000000"
                android:textColor="#ffffffff"
                android:textStyle="bold"
                android:textSize="17dp"
                android:gravity="center"
                android:layout_margin="5dp" />
        </LinearLayout>  
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout4"
            android:gravity="center"
            android:background="#ff8d6e63"
            android:layout_margin="5dp"
            android:layout_gravity="center">           
            <Button
                android:text="@string/Update"
                android:layout_width="100.0dp"
                android:layout_height="70dp"
                android:id="@+id/update"
                android:background="#ff000000"
                android:textColor="#ffffffff"
                android:textStyle="bold"
                android:textSize="17dp"
                android:gravity="center"
                android:layout_margin="5dp" /> 
            <Button
                android:text="@string/Close"
                android:layout_width="101.5dp"
                android:layout_height="70dp"
                android:id="@+id/close"
                android:background="#ff000000"
                android:textColor="#ffffffff"
                android:textStyle="bold"
                android:textSize="17dp"
                android:gravity="center"
                android:layout_margin="5dp" />
                <Button
                android:text="@string/Logout"
                android:layout_width="100.0dp"
                android:layout_height="70dp"
                android:id="@+id/logout"
                android:background="#ff000000"
                android:textColor="#ffffffff"
                android:textStyle="bold"
                android:textSize="17dp"
                android:gravity="center"
                android:layout_margin="5dp" />
        </LinearLayout>    
    </LinearLayout>
</RelativeLayout>